        },
        "handlers.PhoneBookRequest": {
            "type": "object",
            "required": [
                "accountID",
                "name"
            ],
            "properties": {
                "accountID": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "handlers.PhoneBookResponse": {
            "type": "object",
            "properties": {
                "accountID": {
